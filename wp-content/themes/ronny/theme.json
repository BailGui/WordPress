{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "customTemplates": [
    {
      "name": "blank",
      "title": "Blank",
      "postTypes": [
        "page",
        "post"
      ]
    },
    {
      "name": "page-notitle",
      "title": "Page: No title",
      "postTypes": [
        "page"
      ]
    }
  ],
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "name": "header-with-banner",
      "title": "Header With Banner",
      "area": "header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    },
    {
      "area": "uncategorized",
      "name": "sidebar",
      "title": "Sidebar"
    },
    {
      "area": "uncategorized",
      "name": "post-meta",
      "title": "Post Meta"
    }
  ],
  "customTemplates": [
    {
      "name": "page-no-title",
      "postTypes": [
        "page"
      ],
      "title": "Page No Title"
    },
    {
      "name": "page-with-sidebar",
      "postTypes": [
        "page"
      ],
      "title": "Page With Sidebar"
    },
    {
      "name": "page-wide",
      "postTypes": [
        "page"
      ],
      "title": "Page with wide Image"
    },
    {
      "name": "single-fullwidth",
      "postTypes": [
        "post"
      ],
      "title": "Single Fullwidth"
    }
  ],
  "settings": {
    "appearanceTools": true,
    "color": {
      "gradients": [
        {
          "slug": "green-to-yellow",
          "gradient": "linear-gradient(160deg, var(--wp--preset--color--green), var(--wp--preset--color--yellow))",
          "name": "Green to Yellow"
        },
        {
          "slug": "yellow-to-green",
          "gradient": "linear-gradient(160deg, var(--wp--preset--color--yellow), var(--wp--preset--color--green))",
          "name": "Yellow to Green"
        },
        {
          "slug": "red-to-yellow",
          "gradient": "linear-gradient(160deg, var(--wp--preset--color--red), var(--wp--preset--color--yellow))",
          "name": "Red to Yellow"
        },
        {
          "slug": "yellow-to-red",
          "gradient": "linear-gradient(160deg, var(--wp--preset--color--yellow), var(--wp--preset--color--red))",
          "name": "Yellow to Red"
        }
      ],
      "link": true,
      "palette": [
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        },
        {
          "name": "Dark-Gray",
          "slug": "dark-gray",
          "color": "#2b2b2b"
        },
        {
          "name": "Gray",
          "slug": "gray",
          "color": "#f6f6fd"
        },
        {
          "name": "Gray 60",
          "slug": "gray-60",
          "color": "#999999"
        },
        {
          "name": "Scarlet",
          "slug": "scarlet",
          "color": "#cc0100"
        },
        {
          "name": "Ruby",
          "slug": "ruby",
          "color": "#ea1f32"
        },
        {
          "name": "Green",
          "slug": "green",
          "color": "#2A9D8F"
        },
        {
          "name": "Yellow",
          "slug": "yellow",
          "color": "#E9C46A"
        },
        {
          "name": "Orange",
          "slug": "orange",
          "color": "#F4A261"
        },
        {
          "name": "Red",
          "slug": "red",
          "color": "#ffd5cb"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#FFFFFF"
        },
        {
          "name": "Mono",
          "slug": "mono",
          "color": "#E2E8F0"
        },
        {
          "name": "Base",
          "slug": "base",
          "color": "var(--wp--preset--color--white)"
        },
        {
          "name": "Primary",
          "slug": "primary",
          "color": "var(--wp--preset--color--black)"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "var(--wp--preset--color--dark-gray)"
        },
        {
          "name": "Foreground",
          "slug": "foreground",
          "color": "var(--wp--preset--color--black)"
        },
        {
          "name": "Background",
          "slug": "background",
          "color": "var(--wp--preset--color--white)"
        }
      ]
    },
    "layout": {
      "contentSize": "920px",
      "wideSize": "1340px"
    },
    "spacing": {
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "size": "0.625rem",
          "slug": "10",
          "name": "1"
        },
        {
          "size": "1.25rem",
          "slug": "20",
          "name": "2"
        },
        {
          "size": "1.875rem",
          "slug": "30",
          "name": "3"
        },
        {
          "size": "2.5rem",
          "slug": "40",
          "name": "4"
        },
        {
          "size": "3.125rem",
          "slug": "50",
          "name": "5"
        },
        {
          "size": "3.75rem",
          "slug": "60",
          "name": "6"
        },
        {
          "size": "4.375rem",
          "slug": "70",
          "name": "7"
        },
        {
          "size": "5rem",
          "slug": "80",
          "name": "8"
        }
      ],
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "lineHeight": true,
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "Source Serif Pro",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "200 900",
              "src": [
                "file:./assets/fonts/source-serif-pro/SourceSerif4Variable-Roman.ttf.woff2"
              ]
            },
            {
              "fontFamily": "Source Serif Pro",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "200 900",
              "src": [
                "file:./assets/fonts/source-serif-pro/SourceSerif4Variable-Italic.ttf.woff2"
              ]
            }
          ],
          "fontFamily": "\"Source Serif Pro\", serif",
          "name": "Serif",
          "slug": "serif"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "200 900",
              "src": [
                "file:./assets/fonts/inter/Inter-VariableFont_slnt,wght.ttf"
              ]
            }
          ],
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Sans Serif",
          "slug": "sans-serif"
        },
        {
          "fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\", \"Courier New\", monospace",
          "name": "Monospace",
          "slug": "monospace"
        }
      ],
      "fontSizes": [
        {
          "size": "0.875rem",
          "slug": "x-small"
        },
        {
          "size": "1.125rem",
          "slug": "small"
        },
        {
          "size": "1.375rem",
          "slug": "medium"
        },
        {
          "size": "1.625rem",
          "slug": "big"
        },
        {
          "size": "1.875rem",
          "slug": "large"
        },
        {
          "size": "clamp(2.125rem, 3vw, 1.875rem)",
          "slug": "x-large"
        }
      ]
    },
    "useRootPaddingAwareAlignments": true,
    "custom": {
      "typography": {
        "line-height": {
          "body": 1.7,
          "heading": 1.3,
          "page-title": 1.1
        },
        "font-weight": {
          "light": "300",
          "normal": "400",
          "semibold": "500",
          "bold": "700"
        },
        "font-size": {
          "huge": "clamp(2.25rem, 3vw, 2.75rem)",
          "gigantic": "clamp(2.75rem, 4vw, 3.25rem)",
          "colossal": "clamp(3.25rem, 5vw, 4.25rem)"
        }
      }
    }
  },
  "styles": {
    "blocks": {
      "core/site-title": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "none"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--serif)",
          "fontSize": "var(--wp--custom--typography--font-size--huge)",
          "fontWeight": "var(--wp--custom--typography--font-weight--semibold)",
          "lineHeight": "1"
        }
      },
      "core/site-tagline": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "1.4"
        }
      },
      "core/search": {
        "border": {
          "width": "1px",
          "radius": "0px"
        },
        "color": {
          "text": "var(--wp--preset--color--dark-gray)"
        }
      },
      "core/cover": {
        "spacing": {
          "padding": {
            "bottom": "0px",
            "left": "0px",
            "right": "0px",
            "top": "0px"
          }
        }
      },
      "core/post-author": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)",
          "lineHeight": "var(--wp--custom--typography--line-height--body)"
        }
      },
      "core/post-excerpt": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)",
          "lineHeight": "var(--wp--custom--typography--line-height--body)"
        },
        "spacing": {
          "margin": {
            "bottom": "10px",
            "top": "10px"
          }
        }
      },
      "core/post-author-name": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        }
      },
      "core/post-date": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)",
          "lineHeight": "var(--wp--custom--typography--line-height--body)"
        }
      },
      "core/post-title": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        }
      },
      "core/post-terms": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)",
          "lineHeight": "var(--wp--custom--typography--line-height--body)"
        }
      },
      "core/code": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--30)",
            "bottom": "var(--wp--preset--spacing--30)",
            "left": "var(--wp--preset--spacing--30)",
            "right": "var(--wp--preset--spacing--30)"
          }
        },
        "border": {
          "width": "1.5px",
          "radius": "0px",
          "color": "var(--wp--preset--color--dark-gray)",
          "style": "solid"
        }
      },
      "core/categories": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "spacing": {
          "padding": {
            "left": "0px",
            "right": "0px"
          }
        },
        "css": "& {list-style-type:none;} & li{margin-bottom: 0.5rem;}"
      },
      "core/image": {
        "variations": {
          "rounded": {
            "border": {
              "radius": "var(--wp--preset--spacing--30)"
            }
          }
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--white)",
      "text": "var(--wp--preset--color--gray-60)"
    },
    "elements": {
      "button": {
        "border": {
          "width": "1px",
          "radius": "4px"
        },
        "color": {
          "background": "var(--wp--preset--color--scarlet)",
          "text": "var(--wp--preset--color--white)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "var(--wp--custom--typography--font-weight--normal)"
        },
        "spacing": {
          "padding": {
            "bottom": "10px",
            "left": "20px",
            "right": "20px",
            "top": "10px"
          }
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--ruby)",
            "text": "var(--wp--preset--color--white)"
          }
        },
        ":focus": {
          "color": {
            "background": "var(--wp--preset--color--ruby)",
            "text": "var(--wp--preset--color--white)"
          }
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--dark-gray)"
        }
      },
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--serif)",
          "fontSize": "var(--wp--custom--typography--font-size--colossal)",
          "lineHeight": "var(--wp--custom--typography--line-height--page-title)",
          "fontWeight": "var(--wp--custom--typography--font-weight--semibold)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--serif)",
          "fontSize": "var(--wp--custom--typography--font-size--gigantic)",
          "lineHeight": "var(--wp--custom--typography--line-height--heading)",
          "fontWeight": "var(--wp--custom--typography--font-weight--semibold)"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--serif)",
          "fontSize": "var(--wp--custom--typography--font-size--huge)",
          "lineHeight": "var(--wp--custom--typography--line-height--heading)",
          "fontWeight": "var(--wp--custom--typography--font-weight--semibold)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--serif)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "lineHeight": "var(--wp--custom--typography--line-height--heading)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "var(--wp--custom--typography--line-height--heading)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "var(--wp--custom--typography--line-height--heading)"
        }
      }
    },
    "spacing": {
      "blockGap": "1.5rem",
      "padding": {
        "right": "var(--wp--preset--spacing--40)",
        "left": "var(--wp--preset--spacing--40)"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--sans-serif)",
      "fontSize": "var(--wp--preset--font-size--small)",
      "lineHeight": "var(--wp--custom--typography--line-height--body)"
    }
  }
}